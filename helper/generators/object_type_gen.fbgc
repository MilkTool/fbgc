load("file",'*')
load('./examples/string_format.fbgc','format')


f = fopen('deneme.txt','w+')
ftok = fopen('objects.txt','r')


f.fprint('//FBGC object names generated by fbgc\n')

#Now read everything from object.txt and split it with newline, this function will return a tuple of splitted strings
s = ftok.fread() / '\n'

for i = 1:|s|
	str = format('#define <> <>\n', (s[i],i) )
	f.fprint(str)
end

f.fclose()
ftok.fclose()